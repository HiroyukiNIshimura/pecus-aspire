{
  "name": "pecus.frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run generate:conflict-types && npm run generate:help-index && node scripts/generate-pecus-api-client.js && node ../scripts/generate-appsettings.js -D",
    "dev": "next dev",
    "prebuild": "npm run generate:conflict-types && npm run generate:help-index && node scripts/generate-pecus-api-client.js",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --write",
    "format": "biome format --write",
    "full:api": "npm run download:api && npm run generate:api && npm run generate:client",
    "download:api": "curl -o ./.spec/open-api-scheme.json https://localhost:7265/openapi/v1.json",
    "generate:api": "npm run clean:api && openapi --input ./.spec/open-api-scheme.json --output src/connectors/api/pecus --useUnionTypes --client axios && npm run generate:conflict-types",
    "generate:client": "node scripts/generate-pecus-api-client.js",
    "generate:conflict-types": "node scripts/generate-conflict-types.js",
    "generate:help-index": "npx tsx scripts/generate-help-index.ts",
    "clean:api": "rimraf src/connectors/api/pecus",
    "update:check": "ncu",
    "update": "ncu -u"
  },
  "dependencies": {
    "@coati/editor": "*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@excalidraw/excalidraw": "^0.18.0",
    "@floating-ui/react": "^0.27.16",
    "@lexical/clipboard": "0.39.0",
    "@lexical/code": "0.39.0",
    "@lexical/code-shiki": "0.39.0",
    "@lexical/extension": "^0.39.0",
    "@lexical/file": "0.39.0",
    "@lexical/hashtag": "0.39.0",
    "@lexical/html": "0.39.0",
    "@lexical/link": "0.39.0",
    "@lexical/list": "0.39.0",
    "@lexical/mark": "0.39.0",
    "@lexical/markdown": "0.39.0",
    "@lexical/overflow": "0.39.0",
    "@lexical/plain-text": "0.39.0",
    "@lexical/react": "^0.39.0",
    "@lexical/rich-text": "0.39.0",
    "@lexical/selection": "0.39.0",
    "@lexical/table": "0.39.0",
    "@lexical/tailwind": "^0.39.0",
    "@lexical/utils": "0.39.0",
    "@microsoft/signalr": "^10.0.0",
    "@minoru/react-dnd-treeview": "^3.5.3",
    "@yaireo/tagify": "^4.36.0",
    "axios": "^1.13.2",
    "canvas-confetti": "^1.9.4",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "flatpickr": "^4.6.13",
    "flexsearch": "^0.8.212",
    "flyonui": "^2.4.1",
    "ioredis": "^5.9.2",
    "jotai": "^2.16.2",
    "jwt-decode": "^4.0.0",
    "katex": "^0.16.27",
    "lexical": "^0.39.0",
    "lodash-es": "^4.17.22",
    "next": "16.1.3",
    "notyf": "^3.10.0",
    "pristinejs": "^1.1.0",
    "prom-client": "^15.1.3",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dnd": "^16.0.1",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "react-paginate": "^8.3.0",
    "react-select": "^5.10.2",
    "recharts": "^3.6.0",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^6.3.8",
    "use-debounce": "^10.1.0",
