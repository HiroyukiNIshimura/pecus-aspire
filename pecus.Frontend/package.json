{
  "name": "cygnet-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run generate:conflict-types && node scripts/generate-pecus-api-client.js",
    "dev": "next dev",
    "prebuild": "npm run generate:conflict-types && node scripts/generate-pecus-api-client.js",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --write",
    "format": "biome format --write",
    "download:api": "curl -o ./.spec/open-api-scheme.json https://localhost:7265/swagger/v1/swagger.json",
    "generate:conflict-types": "node scripts/generate-conflict-types.js",
    "clean:api": "rimraf src/connectors/api/pecus",
    "generate:api": "npm run clean:api && openapi --input ./.spec/open-api-scheme.json --output src/connectors/api/pecus --useUnionTypes --client axios && npm run generate:conflict-types",
    "generate:client": "node scripts/generate-pecus-api-client.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@minoru/react-dnd-treeview": "^3.5.3",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@yoopta/accordion": "^4.9.9",
    "@yoopta/action-menu-list": "^4.9.9",
    "@yoopta/blockquote": "^4.9.9",
    "@yoopta/callout": "^4.9.9",
    "@yoopta/code": "^4.9.9",
    "@yoopta/divider": "^4.9.9",
    "@yoopta/editor": "^4.9.9",
    "@yoopta/embed": "^4.9.9",
    "@yoopta/file": "^4.9.9",
    "@yoopta/headings": "^4.9.9",
    "@yoopta/image": "^4.9.9",
    "@yoopta/link": "^4.9.9",
    "@yoopta/link-tool": "^4.9.9",
    "@yoopta/lists": "^4.9.9",
    "@yoopta/marks": "^4.9.9",
    "@yoopta/paragraph": "^4.9.9",
    "@yoopta/table": "^4.9.9",
    "@yoopta/toolbar": "^4.9.9",
    "@yoopta/video": "^4.9.9",
    "axios": "^1.13.1",
    "classnames": "^2.5.1",
    "flyonui": "^2.4.1",
    "jotai": "^2.15.1",
    "jwt-decode": "^4.0.0",
    "next": "15.5.4",
    "notyf": "^3.10.0",
    "pristinejs": "^1.1.0",
    "react": "19.1.0",
    "react-dnd": "^16.0.1",
    "react-dom": "19.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-paginate": "^8.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@iconify-json/tabler": "^1.2.23",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-infinite-scroll-component": "^4.2.5",
    "form-data": "^4.0.4",
    "openapi-typescript-codegen": "^0.29.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.17",
    "typescript": "^5"
  }
}
