    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^6.1.19",
    "use-debounce": "^10.0.6",
    "yjs": "^13.6.27",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@iconify-json/tabler": "^1.2.23",
