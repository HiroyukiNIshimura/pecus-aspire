  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/katex": "^0.16.8",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "npm-check-updates": "^19.3.2"
  }
