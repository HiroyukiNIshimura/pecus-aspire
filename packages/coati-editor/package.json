{
  "name": "@coati/editor",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./nodes": {
      "types": "./dist/nodes.d.ts",
      "import": "./dist/nodes.mjs",
      "require": "./dist/nodes.js"
    },
    "./nodes-headless": {
      "types": "./dist/nodes-headless.d.ts",
      "import": "./dist/nodes-headless.mjs",
      "require": "./dist/nodes-headless.js"
    },
    "./transformers": {
      "types": "./dist/transformers.d.ts",
      "import": "./dist/transformers.mjs",
      "require": "./dist/transformers.js"
    },
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check --write",
    "format": "biome format --write",
    "update:check": "ncu",
    "update": "ncu -u"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.17",
